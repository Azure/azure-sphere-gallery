    "version": "0.2.0",
    "configurations": [
        {
            "name": "Native Azure Sphere Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/out/x86-Debug/HelloWorld_HighLevelApp",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        },
