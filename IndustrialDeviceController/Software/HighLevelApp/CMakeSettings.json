{
    "environments": [
      {
        "environment": "AzureSphere",
        "AzureSphereTargetApiSet": "10"
      }
    ],
    "configurations": [
        {
            "name": "MT3620-Release",
            "generator": "Ninja",
            "configurationType": "Release",
            "buildRoot": "${projectDir}\\out\\${name}-${env.AzureSphereTargetApiSet}",
            "installRoot": "${projectDir}\\install\\${name}-${env.AzureSphereTargetApiSet}",
            "cmakeCommandArgs": "--no-warn-unused-cli",
            "buildCommandArgs": "-v",
            "ctestCommandArgs": "",
            "inheritEnvironments": [ "AzureSphere" ],
            "variables": [
                {
                    "name": "CMAKE_TOOLCHAIN_FILE",
                    "value": "${env.AzureSphereDefaultSDKDir}CMakeFiles\\AzureSphereToolchain.cmake",
                    "type": "STRING"
                },
                {
                    "name": "AZURE_SPHERE_TARGET_API_SET",
                    "value": "${env.AzureSphereTargetApiSet}",
                    "type": "STRING"
                },
                {
                    "name": "PRODUCT",
                    "value": "mt3620-idc",
                    "type": "STRING"
                }
            ]
        },
        {
            "name": "MT3620-Debug",
            "generator": "Ninja",
            "configurationType": "Debug",
            "buildRoot": "${projectDir}\\out\\${name}-${env.AzureSphereTargetApiSet}",
            "installRoot": "${projectDir}\\install\\${name}-${env.AzureSphereTargetApiSet}",
            "cmakeCommandArgs": "--no-warn-unused-cli",
            "buildCommandArgs": "-v",
            "ctestCommandArgs": "",
            "inheritEnvironments": [ "AzureSphere" ],
            "variables": [
                {
                    "name": "CMAKE_TOOLCHAIN_FILE",
                    "value": "${env.AzureSphereDefaultSDKDir}CMakeFiles\\AzureSphereToolchain.cmake",
                    "type": "STRING"
                },
                {
                    "name": "AZURE_SPHERE_TARGET_API_SET",
                    "value": "${env.AzureSphereTargetApiSet}",
                    "type": "STRING"
                },
                {
                    "name": "PRODUCT",
                    "value": "mt3620-idc",
                    "type": "STRING"
                }
            ]
        }
    ]
}